SPRING_CACHE_TYPE=redis
SPRING_CACHE_REDIS_TIME_TO_LIVE=3600000
SPRING_CACHE_REDIS_CACHE_NULL_VALUES=true
SPRING_CACHE_REDIS_KEY_PREFIX=smm_cache_
