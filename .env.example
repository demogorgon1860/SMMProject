# ===================================================================

SPRING_SESSION_STORE_TYPE=redis
SPRING_SESSION_REDIS_FLUSH_MODE=on_save
SPRING_SESSION_REDIS_NAMESPACE=spring:session
SPRING_SESSION_TIMEOUT=86400

